extends layout.pug

block content

  nav.nav
    a(href='/')
      span &larr;
      |  Back

  article.portfolio-projects
    .inner-wrapper.flex-row-wrap.two-col
      .project-info-box.box

     

        
        h1 #{ProjectName}

        
        p #{Description}

      .project-tech-links-box.box
        h6 Technologies
        
        ul
          each technologie in technologies
         
      
            li= technologie
         

       
        a.btn-link(href= live) Live Demo
        a.btn-link(href= gitHub) GitHub Repo

    .project-img-box.box

     
      img.project-img(src=imageURL1 alt='1')
      img.project-img(src=imageURL2 alt='2')
      

    nav.nav
      a(href='/')
        span &larr;
        |  Back
